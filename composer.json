{
    "name": "leapt/im-bundle",
    "type": "symfony-bundle",
    "description": "Imagemagick wrapper for Symfony2",
    "keywords": ["image", "thumbnail", "imagemagick", "symfony"],
    "homepage": "https://github.com/leapt/im-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Vanliefland",
            "email": "pierre@snowcap.be"
        },
        {
            "name": "Edwin Hermans",
            "email": "edwin@snowcap.be"
        },
        {
            "name": "Jérôme Poskin",
            "email": "jerome@snowcap.be"
        },
        {
            "name": "Snowcap",
            "homepage": "http://www.snowcap.be"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": ">=2.7",
        "symfony/process": "^2.7||^3.0"
    },
    "require-dev": {
        "symfony/symfony": ">=2.7",
        "mikey179/vfsstream": "v1.0.0",
        "phpunit/phpunit": "~4.7@dev"
    },
    "suggest": {
        "leapt/core-bundle": "ImBundle provides a form type extension for the Leapt CoreBundle ImageType"
    },
    "autoload": {
        "psr-4": { "Leapt\\ImBundle\\": "" }
    }
}
